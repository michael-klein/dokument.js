{
    "name": "dokumentjs",
    "version": "0.0.6",
    "files": [
        "dist"
    ],
    "main": "./dist/dokumentjs.umd.js",
    "module": "./dist/dokumentjs.es.js",
    "exports": {
        ".": {
            "import": "./dist/dokumentjs.es.js",
            "require": "./dist/dokumentjs.umd.js"
        }
    },
    "scripts": {
        "predeploy": "npm run prepare",
        "deploy": "gh-pages -d docs",
        "dev": "vite",
        "build": "vite build && tsc",
        "serve": "vite preview",
        "watch": "vite build --watch",
        "prepare": "npm run build && cp -rf dist docs/dist"
    },
    "dependencies": {
        "history": "^5.0.1",
        "htmdx": "^0.3.6",
        "immer": "^9.0.5",
        "ky": "^0.28.5",
        "preact": "^10.5.13",
        "preact-router": "^3.2.1",
        "react-colorful": "^5.5.0",
        "remove-markdown": "^0.3.0",
        "ss-search": "^1.6.1",
        "zustand": "^3.5.12"
    },
    "devDependencies": {
        "@prefresh/vite": "^2.2.3",
        "@rollup/plugin-alias": "^3.1.5",
        "@types/lodash": "^4.14.172",
        "@types/react": "^17.0.11",
        "sakura.css": "^1.3.1",
        "sass": "^1.42.1",
        "typescript": "^4.4.3",
        "vite": "^2.6.3"
    }
}
